{
  "name": "kissu-tech-interview-back",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "build": "tsc -b",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:migrate:dev": "prisma migrate dev --name init",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset --force",
    "db:reset": "npm run prisma:reset && npm run seed",
    "seed": "tsx src/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop kissu-backend",
    "pm2:restart": "pm2 restart kissu-backend",
    "pm2:reload": "pm2 reload kissu-backend",
    "pm2:delete": "pm2 delete kissu-backend",
    "pm2:logs": "pm2 logs kissu-backend",
    "pm2:monit": "pm2 monit"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.10",
    "@types/supertest": "^6.0.3",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "jest": "^30.1.3",
    "morgan": "^1.10.1",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "prisma": "^6.15.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "ts-node-esm": "^0.0.6",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "zod": "^3.23.8"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "openai": "^5.20.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  }
}